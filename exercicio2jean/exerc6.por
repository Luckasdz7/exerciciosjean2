programa {
  funcao inicio() {
    real a, b, c
    escreva("coloque 3 valores")
    leia(a)
        leia(b)
            leia(c)
            se (a >= b >= c)
            {
              escreva("1")
            }
            senao se (c >= b >= a)
            {
              escreva("-1")
            }
            senao
            {
              escreva("0")
            }


  }
}
